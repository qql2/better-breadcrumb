{
	"name": "better-breadcrumb",
	"version": "0.1.2",
	"description": "This is a sample plugin for Obsidian (https://obsidian.md)",
	"main": "main.js",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc && node esbuild.config.mjs production",
		"version": "node version-bump.mjs && git add manifest.json versions.json",
		"pub": "npm run build & npm publish",
		"build:type": "tsc --declaration --declarationMap --emitDeclarationOnly --isolatedModules false  -outFile index.d.ts"
	},
	"keywords": [],
	"author": "",
	"license": "MIT",
	"dependencies": {
		"vue": "^3.4.38"
	},
	"devDependencies": {
		"@the_tree/esbuild-plugin-vue3": "^0.3.1",
		"@types/node": "^16.18.105",
		"@typescript-eslint/eslint-plugin": "^5.62.0",
		"@typescript-eslint/parser": "^5.62.0",
		"builtin-modules": "^3.3.0",
		"esbuild": "^0.14.54",
		"hash-sum": "^2.0.0",
		"obsidian": "latest",
		"tslib": "2.3.1",
		"typescript": "5.5.4",
		"breadcrumbs": "file:../../../../breadcrumbs"
	},
	"files": [
		"main.js",
		"styles.css",
		"manifest.json",
		"versions.json",
		"index.d.ts",
		"Readme.md"
	]
}